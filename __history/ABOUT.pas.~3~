type
    TAboutBox = class(TForm)
        Panel: TPanel;
        ProgramIcon: TImage;
        sLabel: TsLabel;
        procedure FormDestroy(Sender: TObject);
        procedure FormActivate(Sender: TObject);
    private
    { Private declarations }
    public
    { Public declarations }
    end;

var
    AboutBox: TAboutBox;

implementation

{$R *.dfm}

procedure TAboutBox.FormActivate(Sender: TObject);
begin
    uFMOD_PlaySong(@xm, Length(xm), XM_MEMORY);
end;

end.

